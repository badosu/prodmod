<?xml version="1.0" encoding="utf-8"?>

<!-- wood food metal stone market people player  -->
<object type="text" name="teamMenu" size="0 0 50% 60" textcolor="white" hidden="true" z="90"> <!-- 100% -->

  <repeat count="8" var="i">
    <object name="teamMenuPlayer[i]" type="text" size="0 0 100% 33" hidden="true" sprite="BgBlueDark" z="90"> <!-- 40% -->

      <repeat count="8" var="j">
        <object name="teamMenuPlayer[i]Item[j]" size="0 3 60 33" type="text" hidden="false">
          <object name="teamMenuPlayer[i]Item[j]Btn" size="0 0 30 30" type="button" hidden="false" sprite="BackgroundDarken">
            <object name="teamMenuPlayer[i]Item[j]Icon" type="image" ghost="true"/>
          </object>
          <object name="teamMenuPlayer[i]Item[j]SubBtn" size="24 17 40 33" type="button" hidden="true">
            <object name="teamMenuPlayer[i]Item[j]SubIcon" type="image"/>
          </object>
          <!--  30 3 60 19 -->
          <object name="teamMenuPlayer[i]Item[j]Title" size="28 3 60 19" textcolor="white" type="button"
            hidden="false" font="sans-bold-stroke-12" text_align="left" text_valign="bottom">
          </object>
          <!--  30 18 60 32 -->
          <object name="teamMenuPlayer[i]Item[j]Subtitle" size="28 18 60 32" textcolor="white" type="text"
            hidden="false" font="sans-stroke-12" text_align="left" text_valign="bottom">
          </object>
        </object>
      </repeat>
      <!--
      <object name="teamMenuPlayer[i]Item[7]" size="0 0 200 33" type="text" hidden="false">
